_nipap()
{
	local cur

	COMPREPLY=()
	cur=${COMP_WORDS[COMP_CWORD]}

	if [[ ${#cur} -eq 0 ]]; then
		COMPREPLY=( $( /home/staff/lukagarb/kod/nipap/cli/nipap-helper.py ${COMP_WORDS[@]:0} ''  ) )
	else
		COMPREPLY=( $( /home/staff/lukagarb/kod/nipap/cli/nipap-helper.py ${COMP_WORDS[@]:0} ) )
	fi

	return;

}
complete -F _nipap nipap
